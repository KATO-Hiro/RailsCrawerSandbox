.replayTable[id="replay-world-tour-final"
  data-source="csv/world_tour_final/world_tour_final_2019.csv"
  data-transformer="pointsTable"
  data-insert-start-round="Start"
  data-extra-columns-number="2,3"
  data-columns="position,item,Country,Rating,points,points.change"
  data-labels="#,Handle,Country,Rating,GP30 Points"
  data-animation-speed=0.5
  ]

.replayTable[id="replay-formula-one-drivers-2016"
  data-source="csv/formula-one-drivers.csv"
  data-preset="f1"
  data-extra-columns-number="1"
  data-columns="position,item,Team,points,points.change"
  data-labels="#,Driver,Team,Points"
  ]

.replayTable[id="replay-english-premier-league-2016-2017"
  data-source="json/english-premier-league.json"
